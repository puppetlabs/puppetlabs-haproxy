<%- |
  Array[Variant[String, Sensitive[String]]] $epp_users = [],
  Array[String]                             $epp_groups = [],
  String                                    $epp_section_name,
| -%>

userlist <%= $epp_section_name %>
<%-
  $epp_groups.each |String $group| {
    unless $group.empty {
-%>
  group <%= $group %>
<%-
    }
  }
  $epp_users.each |Variant[String, Sensitive[String]] $user| {
    unless $user.empty {
-%>
  user <%= $user %>
<%-
    }
  }
-%>
